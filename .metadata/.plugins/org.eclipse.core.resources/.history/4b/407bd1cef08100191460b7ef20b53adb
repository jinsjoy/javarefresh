package com.recursion.simple;

public class Fibonacci {
	
static void fibonacii(int first, int second, int n) {
	if(n >= 20) {
		return;
	} else {
	int next = first + second;
	System.out.println(next);
	fibonacii(second, next, ++n);
	}
	
	
}
	
public static void main(String args[]) {
	fibonacii(100);
	
}
}
