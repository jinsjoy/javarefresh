package com.recursion.toi;

public class TowerOfHanoi {
	
	public static int TowerOfHanoiPrint(int n, char A, char B, char C) {
		if(n == 1) {
			return 1;
		} else {
			TowerOfHanoiPrint()
		}
	}
	
	public static void main(String args[]) {
		int n = 4;
		TowerOfHanoiPrint(n,'A','B','C');
	}

}
